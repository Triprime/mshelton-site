<h2>Sign In</h2>

<%= form_tag create_session_path do %>

  <div class='field'>
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %>
  </div>

  <div class='field'>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>

  <div>
  <%= submit_tag "Sign In" %>
  </div>

<% end %>